<h1>Listing asset_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @asset_types.each do |asset_type| %>
  <tr>
    <td><%= asset_type.name %></td>
    <td><%= link_to 'Show', asset_type %></td>
    <td><%= link_to 'Edit', edit_asset_type_path(asset_type) %></td>
    <td><%= link_to 'Destroy', asset_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

